ge": ["9bcd74116f753853ba69f95ef96f361b", "3685c05ff766330187fd41ba4339cc59"], "type": "100"}, {"edge": ["9e9f2697e0653229a2859321e42bffb6", "5bb0615cb02c37e2ae2a9b997856a599"], "type": "100"}, {"edge": ["9e9f2697e0653229a2859321e42bffb6", "e16454e48cb033bb8df8dd7269a77979"], "type": "100"}, {"edge": ["9e9f2697e0653229a2859321e42bffb6", "80619c9bc12a37db983c89b4189270af"], "type": "100"}, {"edge": ["e16454e48cb033bb8df8dd7269a77979", "bf7011f841263f92b8bb23302508607f"], "type": "100"}, {"edge": ["e16454e48cb033bb8df8dd7269a77979", "5d7e4b3f8a3534efbb2cae8838930870"], "type": "100"}, {"edge": ["e16454e48cb033bb8df8dd7269a77979", "2dbe89c689c33a1dae3e5eac77b67d66"], "type": "100"}], "idx": "529:843"}], "code": "/*\n  AOJ 2135\n  Title:Reverse a Road\n  @kankichi573\n*/\n#include <stdio.h>\n#include <string.h>\n\nint N,S,T,M;\nint from[10001],to[10001];\nchar road[1001][1001];\nint distance[2][1001];   // distance from start,end\n\nint dist_all;\n\nvoid calc_distance(int start,int d,int dist[])\n{\n  int i;\n\n  //printf(\"c_d__ %d %d\\n\",start,d);\n\n  if(dist[start]==-1 || dist[start]>d)\n    {\n      dist[start]=d;\n      if(start==T)\n\tdist_all=d;\n    }\n  else\n    return;\n\n  for(i=1;i<=1000;i++)\n    if(road[start][i])\n      calc_distance(i,d+1,dist);\n}\n\nvoid calc_distance_r(int start,int d,int dist[])\n{\n  int i;\n\n  //printf(\"c_d_r %d %d\\n\",start,d);\n\n  if(dist[start]==-1 || dist[start]>d)\n    {\n      dist[start]=d;\n      //if(start==T)\n      //dist_all=d;\n    }\n  else\n    return;\n\n  for(i=1;i<=1000;i++)\n    if(road[i][start])\n      calc_distance_r(i,d+1,dist);\n}\n\nsolve()\n{\n  int i,shortcut,min_,min_r,min_road,d1,d2;\n  \n  for(i=1,min_=dist_all,min_r=0;i<=M;i++)\n    {\n      d1=distance[1][from[i]];\n      d2=distance[0][to[i]];\n      //printf(\"%d:%d %d [%d %d %d]\\n\",i,from[i],to[i],d1,d2,d1+d2+1);\n\n      if(d1 + d2 +1  <dist_all && d1>=0 && d2>=0)\n\t{\n\t  shortcut=(d1+d2+1);\n\t  if(shortcut<min_)\n\t    {\n\t      //printf(\"*+\\n\");\n\t      min_=shortcut;\n\t      min_r=i;\n\t    }\n\t}\n    }\n  if(min_ < dist_all)\n    printf(\"%d %d\\n\",min_, min_r);\n  else\n    printf(\"%d 0\\n\",dist_all);\n\n}\n\nmain()\n{\n  int i,ret,a,b;\n  \n\n  while(scanf(\"%d\",&N)  && (N))\n    {\n      memset(distance,-1,sizeof(distance));\n      memset(road,0,sizeof(road));\n      scanf(\"%d %d\",&S,&T);\n      scanf(\"%d\",&M);\n      for(i=1;i<=M;i++)\n\t{\n\t  scanf(\"%d %d\",&a,&b);\n\t  from[i]=a;\n\t  to[i]=b;\n\t  road[a][b]=1;\n\t}\n      calc_distance(S,0,&distance[0][0]);\n      calc_distance_r(T,0,&distance[1][0]);\n      //printf(\"dall=%d\\n\",dist_all);\n      solve();\n    }\n  return(0);\n}", "label": 0}