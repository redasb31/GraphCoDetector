n; i++) {\n    scanf(\"%lld\", &a[i]);\n  }\n  // Print the maximum number of vertices in the tree\n  printf(\"%lld\\n\", max_vertices(n, a));\n  // Free the allocated memory\n  free(a);\n  return 0;\n}", "label": 1}